  "source": "pragma solidity ^0.4.24;\n\ncontract Image {\n  // Hash # returned by IPFS\n  string ipfsHash;\n \n  // Store the IPFS hash # \n  function sendHash(string _ipfsHash) public {\n    ipfsHash = _ipfsHash;\n  }\n\n  // Return the IPFS hash #\n  function getHash() public view returns (string _ipfsHash) {\n    return ipfsHash;\n  }\n}",
